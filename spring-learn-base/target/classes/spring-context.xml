<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="springBeanTest" class="com.woniu.learn.SpringBeanTest" />

    <bean id="apiService" class="com.spring.mockito.ApiService">
        <property name="serviceName" value="wang"/>
    </bean>
    <bean id="testApiService" class="com.spring.mockito.TestApiService">
        <property name="name" value="testApiServiceName"/>
    </bean>

    <bean id="testApiDao" class="com.spring.mockito.TestApiDao" />
    <bean id="testApiManager" class="com.spring.mockito.TestApiManager" />
    <bean id="otherService" class="com.spring.mockito.OtherService">
        <property name="name" value="otherService" />
    </bean>

</beans>